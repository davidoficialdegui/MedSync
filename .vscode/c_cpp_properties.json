{
  "version": 4,
  "configurations": [
    {
      "name": "windows-gcc-x86",
      "compilerPath": "C:/MinGW/bin/gcc.exe",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x86",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/src",
          "${workspaceFolder}/src/**"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}